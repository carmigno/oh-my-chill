{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "\u256d\u2500<#F4E4BC>{{ .UserName }}@{{ .HostName }}</>\u2500",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "style": "full"
          },
          "template": "<#87CEEB>[{{ .Path }}]</>",
          "foreground": "#87CEEB",
          "type": "path",
          "style": "plain"
        },
        {
          "template": "<#FFDAB9>\u2500(</>#<#F5F5F5>)</>",
          "foreground": "#FFDAB9",
          "type": "root",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " <#F08080>\ue5fa</> ",
            "yarn_icon": " <#7BB3F0>\ue6a7</>"
          },
          "template": "<#F5F5F5>(</>{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}<#F5F5F5>)</>",
          "foreground": "#8FBC8F",
          "type": "node",
          "style": "plain"
        },
        {
          "template": "{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "foreground": "#F0E68C",
          "type": "python",
          "style": "plain"
        },
        {
          "template": "<#F5F5F5>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#F5F5F5>)</>",
          "foreground": "#F08080",
          "type": "java",
          "style": "plain"
        },
        {
          "template": "<#F5F5F5>(</>{{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }}<#F5F5F5>)</>",
          "foreground": "#7BB3F0",
          "type": "dotnet",
          "style": "plain"
        },
        {
          "template": "<#F5F5F5>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#F5F5F5>)</>",
          "foreground": "#AFEEEE",
          "type": "go",
          "style": "plain"
        },
        {
          "template": "<#F5F5F5>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#F5F5F5>)</>",
          "foreground": "#DEB887",
          "type": "rust",
          "style": "plain"
        },
        {
          "template": "<#F5F5F5>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#F5F5F5>)</>",
          "foreground": "#7BB3F0",
          "type": "dart",
          "style": "plain"
        },
        {
          "template": "<#F5F5F5>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#F5F5F5>)</>",
          "foreground": "#F08080",
          "type": "angular",
          "style": "plain"
        },
        {
          "template": "<#F5F5F5>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#F5F5F5>)</>",
          "foreground": "#B19CD9",
          "type": "aurelia",
          "style": "plain"
        },
        {
          "template": "<#333333>(</>{{ if .Error }}{{ .Error }}{{ else }}Nx {{ .Full }}{{ end }}<#333333>)</>",
          "foreground": "#F5F5F5",
          "type": "nx",
          "style": "plain"
        },
        {
          "template": "<#F5F5F5>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#F5F5F5>)</>",
          "foreground": "#8FBC8F",
          "type": "julia",
          "style": "plain"
        },
        {
          "template": "<#F5F5F5>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#F5F5F5>)</>",
          "foreground": "#F08080",
          "type": "ruby",
          "style": "plain"
        },
        {
          "template": "<#F5F5F5>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#F5F5F5>)</>",
          "foreground": "#7BB3F0",
          "type": "azfunc",
          "style": "plain"
        },
        {
          "template": "<#F5F5F5>(</>{{.Profile}}{{if .Region}}@{{.Region}}{{end}}<#F5F5F5>)</>",
          "foreground": "#FFDAB9",
          "type": "aws",
          "style": "plain"
        },
        {
          "template": "<#F5F5F5>(</>{{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}<#F5F5F5>)</>",
          "foreground": "#7BB3F0",
          "type": "kubectl",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "\u2570\u2500",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": "<#F5F5F5>[</>{{ .HEAD }}{{ if .Staging.Changed }}<#98FB98> \u25cf {{ .Staging.String }}</>{{ end }}{{ if .Working.Changed }}<#FFDAB9> \u25cf {{ .Working.String }}</>{{ end }}<#F5F5F5>]-</>",
          "foreground": "#E0F6FF",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": true
          },
          "template": "\ue285 ",
          "foreground": "#98FB98",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#FFB6C1{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
