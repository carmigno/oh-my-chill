{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "\u256d\u2500<#D2B48C>{{ .UserName }}@{{ .HostName }}</>\u2500",
          "type": "session"
        },
        {
          "foreground": "#A67B5B",
          "style": "plain",
          "properties": {
            "style": "full"
          },
          "template": "<#A67B5B>[{{ .Path }}]</>",
          "type": "path"
        },
        {
          "foreground": "#C7A882",
          "style": "plain",
          "template": "<#C7A882>\u2500(</>#<#C8B299>)</>",
          "type": "root"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#8B6F47",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " <#B8956B>\ue5fa</> ",
            "yarn_icon": " <#A67B5B>\ue6a7</>"
          },
          "style": "plain",
          "template": "<#C8B299>(</>{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}<#C8B299>)</>",
          "type": "node"
        },
        {
          "foreground": "#D4B896",
          "style": "plain",
          "template": "{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "type": "python"
        },
        {
          "foreground": "#B8956B",
          "style": "plain",
          "template": "<#C8B299>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#C8B299>)</>",
          "type": "java"
        },
        {
          "foreground": "#947A61",
          "style": "plain",
          "template": "<#C8B299>(</>{{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }}<#C8B299>)</>",
          "type": "dotnet"
        },
        {
          "foreground": "#8B7355",
          "style": "plain",
          "template": "<#C8B299>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#C8B299>)</>",
          "type": "go"
        },
        {
          "foreground": "#7D6342",
          "style": "plain",
          "template": "<#C8B299>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#C8B299>)</>",
          "type": "rust"
        },
        {
          "foreground": "#BFA083",
          "style": "plain",
          "template": "<#C8B299>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#C8B299>)</>",
          "type": "dart"
        },
        {
          "foreground": "#A67B5B",
          "style": "plain",
          "template": "<#C8B299>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#C8B299>)</>",
          "type": "angular"
        },
        {
          "foreground": "#C7A882",
          "style": "plain",
          "template": "<#C8B299>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#C8B299>)</>",
          "type": "aurelia"
        },
        {
          "foreground": "#C8B299",
          "style": "plain",
          "template": "<#8B7355>(</>{{ if .Error }}{{ .Error }}{{ else }}Nx {{ .Full }}{{ end }}<#8B7355>)</>",
          "type": "nx"
        },
        {
          "foreground": "#D4B896",
          "style": "plain",
          "template": "<#C8B299>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#C8B299>)</>",
          "type": "julia"
        },
        {
          "foreground": "#6B5B3F",
          "style": "plain",
          "template": "<#C8B299>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#C8B299>)</>",
          "type": "ruby"
        },
        {
          "foreground": "#947A61",
          "style": "plain",
          "template": "<#C8B299>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#C8B299>)</>",
          "type": "azfunc"
        },
        {
          "foreground": "#C7A882",
          "style": "plain",
          "template": "<#C8B299>(</>{{.Profile}}{{if .Region}}@{{.Region}}{{end}}<#C8B299>)</>",
          "type": "aws"
        },
        {
          "foreground": "#8B6F47",
          "style": "plain",
          "template": "<#C8B299>(</>{{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}<#C8B299>)</>",
          "type": "kubectl"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "style": "plain",
          "template": "\u2570\u2500",
          "type": "text"
        },
        {
          "foreground": "#BFA083",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "<#C8B299>[</>{{ .HEAD }}{{ if .Staging.Changed }}<#8B6F47> \u25cf {{ .Staging.String }}</>{{ end }}{{ if .Working.Changed }}<#A67B5B> \u25cf {{ .Working.String }}</>{{ end }}<#C8B299>]-</>",
          "type": "git"
        },
        {
          "foreground": "#D2B48C",
          "foreground_templates": ["{{ if gt .Code 0 }}#B8956B{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\ue285 ",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 1
}

