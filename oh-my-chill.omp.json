{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "style": "plain",
          "template": "\u256d\u2500<#FFEB3B>{{ .UserName }}@{{ .HostName }}</>\u2500",
          "type": "session"
        },
        {
          "foreground": "#64B5F6",
          "style": "plain",
          "properties": {
            "style": "full"
          },
          "template": "<#64B5F6>[{{ .Path }}]</>",
          "type": "path"
        },
        {
          "foreground": "#FF9800",
          "style": "plain",
          "template": "<#FF9800>\u2500(</>#<#ffffff>)</>",
          "type": "root"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#81C784",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " <#E57373>\ue5fa</> ",
            "yarn_icon": " <#4FC3F7>\ue6a7</>"
          },
          "style": "plain",
          "template": "<#ffffff>(</>{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}<#ffffff>)</>",
          "type": "node"
        },
        {
          "foreground": "#C0CA33",
          "style": "plain",
          "template": "{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "type": "python"
        },
        {
          "foreground": "#F06292",
          "style": "plain",
          "template": "<#ffffff>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "type": "java"
        },
        {
          "foreground": "#64B5F6",
          "style": "plain",
          "template": "<#ffffff>(</>{{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "type": "dotnet"
        },
        {
          "foreground": "#4FC3F7",
          "style": "plain",
          "template": "<#ffffff>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "type": "go"
        },
        {
          "foreground": "#BCAAA4",
          "style": "plain",
          "template": "<#ffffff>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "type": "rust"
        },
        {
          "foreground": "#7986CB",
          "style": "plain",
          "template": "<#ffffff>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "type": "dart"
        },
        {
          "foreground": "#E57373",
          "style": "plain",
          "template": "<#ffffff>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "type": "angular"
        },
        {
          "foreground": "#BA68C8",
          "style": "plain",
          "template": "<#ffffff>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "type": "aurelia"
        },
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<#1e293b>(</>{{ if .Error }}{{ .Error }}{{ else }}Nx {{ .Full }}{{ end }}<#1e293b>)</>",
          "type": "nx"
        },
        {
          "foreground": "#8BC34A",
          "style": "plain",
          "template": "<#ffffff>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "type": "julia"
        },
        {
          "foreground": "#F06292",
          "style": "plain",
          "template": "<#ffffff>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "type": "ruby"
        },
        {
          "foreground": "#64B5F6",
          "style": "plain",
          "template": "<#ffffff>(</>{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}<#ffffff>)</>",
          "type": "azfunc"
        },
        {
          "foreground": "#FF8A65",
          "style": "plain",
          "template": "<#ffffff>(</>{{.Profile}}{{if .Region}}@{{.Region}}{{end}}<#ffffff>)</>",
          "type": "aws"
        },
        {
          "foreground": "#7986CB",
          "style": "plain",
          "template": "<#ffffff>(</>{{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}<#ffffff>)</>",
          "type": "kubectl"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "style": "plain",
          "template": "\u2570\u2500",
          "type": "text"
        },
        {
          "foreground": "#81D4FA",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "<#ffffff>[</>{{ .HEAD }}{{ if .Staging.Changed }}<#81C784> \u25cf {{ .Staging.String }}</>{{ end }}{{ if .Working.Changed }}<#FF9800> \u25cf {{ .Working.String }}</>{{ end }}<#ffffff>]-</>",
          "type": "git"
        },
        {
          "foreground": "#B3E5FC",
          "foreground_templates": ["{{ if gt .Code 0 }}#E57373{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\ue285 ",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 1
}

